/* pages/ResBase/ResBase.wxss */
page{
    background-color: #e9ece9;
}
.main{
    background-color: #fff;
}
.list-picture {
    width: 94%;
    height: 80px;
    line-height: 80px;
    padding: 15px 3%;
    background-color: #fff;
}
.list-picture-left {
    float: left;
    width: 25%;
    height: 80px;
    line-height: 80px;
    color: #404040;
    font-size: 15px;
}
.res-headImg {
    float: right;
    text-align: center;
    border-radius: 100%;
    height: 50px;
    line-height: 50px;
    width: 50px;
    margin-top: 15px;
    background-color: #f2f2f2!important;
    margin-right: 5%;
    background-size: 100% 100%;
}
.radio-group{
    text-align: right;
}
.radio{
    font-size: 15px;
    color: #9b9b9b;
    margin-left: 30px;
}
.radio-type{
    transform:scale(.8);
}
.placStyle{
   color: #9b9b9b;
   font-size: 15px;
}